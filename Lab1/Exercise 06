<script src='https://d3js.org/d3.v7.min.js'></script> 
 
<div></div> 
<div></div> 
<div></div> 
<div></div> <!--adding an extra div so the text method iterates over the new variable-->
 
<script> 
let otherdata = [ {name:'test',  val:1},          
                  {name:'other', val:2}, 
                  {name:'b',     val:3},
                  {name:'color', val:'green'}];  //variable color with value green added
 
let paragraph = d3.select("body") 
                .selectAll("div") 
                .data(otherdata) 
                .text(function (d, i) { 
                    console.log("d.name: " + d.name); //the structure is the same but one new variable is added
                    console.log("d.val:  " + d.val); 
                    console.log("i: " + i); 
                    console.log("this: " + this); 
 
                    return 'cont:' + d.name +"  -->  value: "+ d.val // return value is used to set the 'text' 
                }); 
</script> 
